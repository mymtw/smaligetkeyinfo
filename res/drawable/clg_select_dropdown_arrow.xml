<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="true" android:id="@id/checked" android:drawable="@drawable/$clg_select_dropdown_arrow__0" />
    <item android:id="@id/unchecked" android:drawable="@drawable/clg_select_dropdown_arrow_down" />
    <transition android:drawable="@drawable/clg_select_dropdown_arrow_down_to_up" android:toId="@id/checked" android:fromId="@id/unchecked" />
    <transition android:drawable="@drawable/clg_select_dropdown_arrow_up_to_down" android:toId="@id/unchecked" android:fromId="@id/checked" />
</animated-selector>