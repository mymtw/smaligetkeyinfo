<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false" android:state_checked="false" android:drawable="@drawable/$clg_checkbox_selector__0" />
    <item android:state_checked="false" android:id="@id/unchecked" android:drawable="@drawable/$clg_checkbox_selector__1" />
    <item android:state_enabled="false" android:state_checked="true" android:drawable="@drawable/$clg_checkbox_selector__2" />
    <item android:state_checked="true" android:id="@id/checked" android:drawable="@drawable/$clg_checkbox_selector__3" />
    <transition android:drawable="@drawable/clg_checkbox_toggle_unchecked_checked" android:toId="@id/checked" android:fromId="@id/unchecked" />
    <transition android:drawable="@drawable/clg_checkbox_toggle_checked_unchecked" android:toId="@id/unchecked" android:fromId="@id/checked" />
</animated-selector>