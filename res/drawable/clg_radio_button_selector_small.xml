<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false" android:state_checked="false" android:drawable="@drawable/$clg_radio_button_selector_small__0" />
    <item android:state_checked="false" android:id="@id/unchecked" android:drawable="@drawable/$clg_radio_button_selector_small__1" />
    <item android:state_enabled="false" android:state_checked="true" android:drawable="@drawable/$clg_radio_button_selector_small__2" />
    <item android:state_checked="true" android:id="@id/checked" android:drawable="@drawable/$clg_radio_button_selector_small__3" />
    <transition android:drawable="@drawable/clg_radiobutton_toggle_unchecked_checked_small" android:toId="@id/checked" android:fromId="@id/unchecked" />
    <transition android:drawable="@drawable/clg_radiobutton_toggle_checked_unchecked_small" android:toId="@id/unchecked" android:fromId="@id/checked" />
</animated-selector>